<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dressassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HairDetails"></activity>
        <activity android:name=".HairStyle"></activity>
        <activity android:name=".DressDetails"></activity>
        <activity android:name=".DressStyle"></activity>
        <activity android:name=".MakeupStyle"></activity>
        <activity android:name=".MakeupDetails"></activity>
        <activity android:name=".MakeupStar"></activity>
        <activity android:name=".MySpace"></activity>
        <activity android:name=".MyCollect"></activity>
        <activity android:name=".MyClass"></activity>
        <activity android:name=".Login"></activity>
        <activity android:name=".Register"></activity>
    </application>

</manifest>